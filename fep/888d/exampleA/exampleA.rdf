<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:DatatypeProperty rdf:about="https://w3id.org/fep/888d/exampleA">
    <rdfs:label xml:lang="en">exampleA</rdfs:label>
    <rdfs:comment xml:lang="en">A property with some literal value (string, boolean, integer)</rdfs:comment>
    <rdfs:domain rdf:resource="https://w3id.org/fep/888d/SomeType"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:range>

  </owl:DatatypeProperty>

</rdf:RDF>